<div class="container">
  <h1>Q<%= @question.question_num %></h1>
  <p><%= @question.question_content %></p>
  <div class=top-button>
    <%= link_to '前へ', question_path(@question.previous) if @question.previous %>
    <%= link_to @question.next ? '次へ' : '完了', @question.next ? question_path(@question.next) : user_path(current_user.id) %>
  </div>
</div>
